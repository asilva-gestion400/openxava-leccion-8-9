<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="Facturacion"> 		<modulo-defecto>		  		<controlador nombre="Typical"/>			</modulo-defecto>		<modulo nombre="FacturasPagadas">		<modelo nombre="Factura"/>		<tab nombre="facturasPagadas"/>		<controlador nombre="FacturasPagadas"/>	</modulo>		<modulo nombre="FacturasPendientes">		<modelo nombre="Factura"/>		<tab nombre="facturasPendientes"/>		<controlador nombre="Typical"/>	</modulo>		<modulo nombre="Factura">		<modelo nombre="Factura"/>		<controlador nombre="TypicalNotResetOnSave"/>		<controlador nombre="Factura"/>	</modulo>		<modulo nombre="FacturaSoloDetalle">		<modelo nombre="Factura"/>	    <controlador nombre="Typical"/>	    <controlador-modo nombre="Void"/> 	</modulo>		<modulo nombre="FacturaSoloLista">		<var-entorno nombre="XAVA_LIST_ACTION" valor=""/>    	    <modelo nombre="Factura"/>	    <controlador nombre="Typical"/>	    <controlador-modo nombre="ListOnly"/>  	</modulo>		<modulo nombre="FacturaSoloLectura">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="CRUD.searchReadOnly"/>    	    <modelo nombre="Factura"/>	    <controlador nombre="Print"/>	</modulo>		<modulo nombre="FacturaSoloLectura2">		<var-entorno nombre="XAVA_SEARCH_ACTION" valor="CRUDReimplementado.soloLectura"/>    	    <modelo nombre="Factura"/>	    <controlador nombre="Print"/>	</modulo>		</aplicacion>